  <body>

    <div id="wrapper">
      <header>
        <a href="/" id="eclipse-scout-logo"></a>

        <nav id="main-navigation">
          <div class="navigation-item lv1 first"><a href="#" class="text">Tutorial</a></div>
          <div class="navigation-item lv1"><a href="#" class="text">Docs</a></div>
